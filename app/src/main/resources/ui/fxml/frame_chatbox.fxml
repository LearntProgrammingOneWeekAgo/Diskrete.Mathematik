<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="370.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.ui.GUI_Controller_Chatbox">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="279.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
      <children>
            <TextField fx:id="input_box" layoutX="14.0" layoutY="334.0" onKeyPressed="#handleEnter" prefHeight="26.0" prefWidth="546.0" promptText="Hallo Welt..." />
            <Button fx:id="send_button" layoutX="570.0" layoutY="334.0" mnemonicParsing="false" onAction="#senden" prefHeight="27.0" prefWidth="56.0" text="Senden" />
            <TextArea fx:id="output_field" editable="false" layoutX="14.0" layoutY="14.0" prefHeight="307.0" prefWidth="610.0" />
      </children>
    </AnchorPane>
  </children>
</VBox>